{
    "name": "shrikeh/rob-hawkins-my-first-web-page",
    "description": "Watch as Rob takes his first giant leap into Web Programming! Ages 5+",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "symfony/cache": "^5.0",
        "symfony/console": "^5.0",
        "symfony/dotenv": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^5.0",
        "symfony/http-client": "^5.0",
        "symfony/http-client-contracts": "^2.0",
        "symfony/messenger": "^5.0",
        "symfony/yaml": "^5.0",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "behat/behat": "^3.5@dev",
        "codeception/codeception": "4.0.x-dev",
        "codeception/module-asserts": "^1.0.0",
        "codeception/module-symfony": "dev-master",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.2",
        "friends-of-behat/symfony-extension": "^2.1@dev",
        "friends-of-phpspec/phpspec-code-coverage": "^4.0@dev",
        "infection/infection": "^0.15.0",
        "jangregor/phpstan-prophecy": "dev-master",
        "jetbrains/phpstorm-stubs": "dev-master",
        "nikic/php-parser": "^4.3",
        "overtrue/phplint": "^1.2",
        "php-coveralls/php-coveralls": "2.2.x-dev",
        "php-ds/php-ds": "dev-master",
        "phpcompatibility/php-compatibility": "dev-master",
        "phpmd/phpmd": "dev-master",
        "phpmetrics/phpmetrics": "dev-master",
        "phpspec/phpspec": "^6.1",
        "phpstan/extension-installer": "dev-master",
        "phpstan/phpstan": "^0.12.0@dev",
        "phpstan/phpstan-phpunit": "^0.12.0@dev",
        "phpunit/phpunit": "^8.5",
        "rector/rector": "^0.6",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/thanks": "^1.2",
        "vimeo/psalm": "^3.9"
    },
    "autoload": {
        "RobHawkins\\MyFirstWebPage\\": "src"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\RobHawkins\\MyFirstWebPage\\": "tests/unit/src"
        },
        "classmap": [
            "tests/Constants.php"
        ]
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
